    "postinstall": "npm rebuild nodegit",
    "clean": "rm -rf dist",
    "build": "NODE_ENV=production run-s build:*",
    "build:js": "webpack --mode development --progress",
    "watch": "run-p watch:*",
    "watch:js": "NODE_ENV=development webpack --mode development --progress -w",
    "watch:server": "NODE_ENV=development node inspect app",
    "start": "NODE_ENV=production node app",
    "lint": "eslint app constants frontend migrations models seeders shared",
    "typecheck": "flow",
    "typecheck:coverage": "flow-coverage-report -i '{app,constants,frontend,migrations,models,seeders,shared}/**/*.js' -x '*/**/*.test.js'",
    "test": "run-s test:tape test:jest",
    "test:tape": "NODE_ENV=test tape -r @babel/register -r @babel/polyfill '{app,frontend,shared}/**/*.legacy-test.js' 2>/dev/null | tap-diff",
    "test:jest": "NODE_ENV=test jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
